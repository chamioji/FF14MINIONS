<% if current_page.last? %>
  <li class="disabled">
    <%= content_tag :a do %>
      <i class="material-icons">last_page</i>
    <% end %>
  </li>
<% else %>
  <li class="waves-effect">
    <%= link_to url, :rel => 'last', :remote => remote do %>
      <i class="material-icons">last_page</i>
    <% end %>
  </li>
<% end %>
